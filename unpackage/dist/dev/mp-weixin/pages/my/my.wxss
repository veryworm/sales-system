.content {
  background-color: #F2F2F2;
  padding-bottom: 10px;
}
.status_bar {
  height: 25px;
  width: 100%;
}
.my_header {
  width: 100%;
  padding: 5px 10px 20px 10px;
  background-image: url(data:image/jpeg;base64,);
}
.my_image {
  border-radius: 50%;
  float: left;
  width: 18%;
  height: 60px;
  background-color: #007AFF;
}
.my_header::after {
  content: "";
  display: block;
  clear: both;
}
.my_name {
  color: #FFFFFF;
  margin-left: 80px;
  font-size: 16px;
}
.my_order_total {
  margin-top: 10px;
  background-color: #FFFFFF;
  padding: 10px;
  border-radius: 10px;
}
.my_order_total > ._ul {
  list-style: none;
  margin: 0;
  padding: 0;
}
.my_order_total > ._ul::after {
  content: "";
  display: block;
  clear: both;
}
.my_order_total > ._ul > ._li {
  float: left;
  width: 18%;
  font-size: 12px;
  margin-left: 20px;
  text-align: center;
  line-height: 1.5em;
}
.other_message {
  margin-top: 10px;
  background-color: #FFFFFF;
  padding: 10px;
  border-radius: 10px;
}

