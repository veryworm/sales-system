}
.card_logo>._li{
	background-color: #FFFFFF;
	border: 1px solid #FFFFFF;
	text-align: center;
	display: inline-block;
	width:75px;
	height: 30px;
	margin: 0 auto;
	text-align: center;
	margin-top: 20px;
}
